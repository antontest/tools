# Name : ecmake auto complete
# Func : 
# Time : 2015-10-13 13:18:33

have ipsec-tools && {
_ipsec_tools()
{
    local cur prev opts ca_save_file certs_save_file
    cur=`_get_cword`
    prev=${COMP_WORDS[COMP_CWORD-1]}
    COMPREPLY=()

    if [[ "$cur" = -* ]] ; then
        COMPREPLY=( $( compgen -W '--inform --showcert --printcert --get-issuer --get-subject --get-serial --get-ocsp --get-validity --in -h --help' -- "$cur" ) )
        return 0
    fi
 
    if [[ "$prev" = "--inform" ]] ; then
        COMPREPLY=( $( compgen -W 'pem' -- "$cur" ) )
        return 0
    fi

    _expand || return 0
    COMPREPLY=( $( compgen -f -X ".*" -- "$cur" ) \
                $( compgen -d -- "$cur" ) )
    return 0
}

complete -F _ipsec_tools $filenames ipsec-tools
}
